{
  "name": "mm-developer-portal-react",
  "version": "1.0.0",
  "private": true,
  "description": "mm-developer-portal-react",
  "author": "Samuel Parkinson",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "build:prod": "gatsby build --prefix-paths"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,yml,yaml,css,scss,md,mdx,html}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@mott-macdonald/smi-react-ui-kit": "^2.8.0",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "babel-plugin-import": "^1.13.5",
    "gatsby": "^4.24.1",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-material-ui": "^4.1.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-copy-linked-files": "^5.24.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^6.24.0",
    "gatsby-remark-prismjs": "^6.24.0",
    "gatsby-remark-smartypants": "^5.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-remark": "^5.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "gatsby-transformer-yaml": "^4.24.0",
    "prism-themes": "^1.9.0",
    "rambdax": "^8.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rehype-react": "^7.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-preset-gatsby": "^2.24.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.7.2",
    "eslint-webpack-plugin": "^3.2.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.3"
  }
}
